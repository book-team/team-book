            <view style="background:#fff; {{data.contentstyle}}">
                <view class="grade-wrap">
                    <label class="ico-moon icon-{{data.icon == 'love' ? 'heart' : (data.icon == 'plum' ? 'clubs' : data.icon)}}{{ data.editScore >= 1 ? '' : '-empty' }}" data-compid="{{data.compId}}" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}"
