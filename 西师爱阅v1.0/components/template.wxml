<!--动态表单--选项-->
<template name="vqdSelectEle">
    <block wx:if="{{data.namestyle}}">
        <view class="newoption_sub clearfix" style="{{data.style}}">
            <text class="single_text_span" style="{{data.namestyle}}"><text class="form_must" wx:if="{{data.isneed==2}}" style="left:-10rpx;">*</text>{{data.content.title}}</text>
            <view class="clearfix" class="{{data.choosenum==0?'option_sub_ul1':''}} {{data.choosenum==1?'option_sub_ul2':''}}" style="{{data.contentstyle}}">
                <checkbox-group class="checkbox-group" wx:if="{{data.selectAmount > 1}}" bindchange="bindSelectChange" data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" data-selectnum="{{data.selectAmount}}" data-compid="{{data.compId}}"
                    data-formcompid="{{data.formCompid}}">
                    <view class="select-item {{data.choosenum==0?'option1':'option2'}}" wx:for="{{data.content.lists}}" wx:key="{{item}}">
                        <label class="select-content checkbox">
                            <checkbox class="select-input" value="{{item}}" />
                            <label class="select-text">{{item}}</label>
                        </label>
                    </view>
                </checkbox-group>
                <radio-group class="radio-group" bindchange="bindSelectChange" wx:else data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}">
                    <view class="select-item {{data.choosenum==0?'option1':'option2'}}" wx:for="{{data.content.lists}}" wx:key="{{item}}">
                        <label class="select-content checkbox">
                            <radio class="select-input" value="{{item}}" />
                            <label class="select-text">{{item}}</label>
                        </label>
                    </view>
                </radio-group>
            </view>
        </view>
    </block>
    <block wx:else>
        <view class="element select-ele vqd-form-ele" style="{{data.style}}">
            <view class="select-container">
                <label class="title-container" style='{{data.titleStyle}}'>
                    <text class="title"><text class="form_must" wx:if="{{data.isneed==2}}">*</text>{{data.content.title}}</text>
                </label>
                <checkbox-group class="checkbox-group" wx:if="{{data.selectAmount > 1}}" bindchange="bindSelectChange" data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" data-selectnum="{{data.selectAmount}}" data-compid="{{data.compId}}"
                    data-formcompid="{{data.formCompid}}">
                    <view class="select-item" wx:for="{{data.content.lists}}" wx:key="{{item}}">
                        <label class="select-content checkbox">
                            <label class="select-text">{{item}}</label>
                            <checkbox class="select-input" value="{{item}}" />
                        </label>
                    </view>
                </checkbox-group>
                <radio-group class="radio-group" bindchange="bindSelectChange" wx:else data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}">
                    <view class="select-item" wx:for="{{data.content.lists}}" wx:key="{{item}}">
                        <label class="select-content checkbox">
                            <label class="select-text">{{item}}</label>
                            <radio class="select-input" value="{{item}}" />
                        </label>
                    </view>
                </radio-group>
            </view>
        </view>
    </block>
</template>
<!--动态表单--上传图片-->
<template name="vqdUploadImg">
    <block wx:if="{{data.namestyle}}">
        <view class="newupload_img_sub" style="{{data.style}}">
            <text class="upload_image" style="{{data.namestyle}}"><text class="form_must" wx:if="{{data.isneed==2}}">*</text>{{data.textname}}</text>
            <view class="uploadimg_con" style="{{data.listyle}}">
                <image class="upload_pic" src="{{data.piccontent}}" style="{{data.contentstyle}}"></image>
                <view class="newplus_div" catchtap="uploadFormImg" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}" data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" style="{{data.btnstyle}}">
                    <text>＋</text>
                </view>
            </view>
        </view>
    </block>
    <block wx:else>
        <block wx:if="{{data.display_upload != false}}">
            <view class="element upload-img vqd-form-ele" style="{{data.style}}">
                <view class="plus-div" catchtap="uploadFormImg" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}" data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}">
                    <text>＋</text>
                </view>
            </view>
        </block>
        <block wx:else>
            <view style="text-align:center;">
                <image lazy-load="true" src="{{data.content}}" style="{{data.style}}" mode="aspectFit" catchtap="uploadFormImg" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}" data-segment="{{data.segment}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}"></image>
            </view>
        </block>
    </block>
</template>
<!--动态表单--时间组件-->
<template name="vqdTimeEle">
    <block wx:if="{{data.namestyle}}">
        <view class="element xcx-time-ele" style="{{data.style}}">
            <text class="single_text_span" style="{{data.namestyle}}"><text class="form_must" wx:if="{{data.isneed==2}}">*</text>{{data.textname}}</text>
            <view style="{{data.contentstyle}} line-height:normal;">
                <text class="timename" wx:if="{{data.timename!=''}}">{{data.timename}}</text>
                <picker mode="date" bindchange="bindDateChange" style="width:50%;display:inline-block;text-align:right;" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-compid="{{data.compId}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}">
                    <label class="date-btn {{data.timename!=''?'new-date-btn':''}}" style="width: 100%;">{{data.date? data.date: "请选择日期"}}</label>
                </picker>
                <picker mode="time" bindchange="bindTimeChange" style="width:50%;display:inline-block;text-align:left;" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-compid="{{data.compId}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}">
                    <label class="time-btn {{data.timename!=''?'new-date-btn':''}}" style="width: 100%;">{{data.time? data.time: "请选择时间"}}</label>
                </picker>
            </view>
        </view>
    </block>
    <block wx:else>
        <view class="element xcx-time-ele">
            <text class="timename">{{data.timename}}</text>
            <picker mode="date" bindchange="bindDateChange" style="width:50%;display:inline-block;text-align:right;" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-compid="{{data.compId}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}">
                <label class="date-btn">{{data.date? data.date: "请选择日期"}}</label>
            </picker>
            <picker mode="time" bindchange="bindTimeChange" style="width:50%;display:inline-block;text-align:left;" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-compid="{{data.compId}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}">
                <label class="time-btn">{{data.time? data.time: "请选择时间"}}</label>
            </picker>
        </view>
    </block>
</template>
<!--动态表单--提交按钮-->
<template name="vqdFormButton">
    <button class="button background-ele" style="{{data.style}}" form-type="submit">
        {{data.content}}
    </button>
</template>
<!--悬浮按钮-->
<template name="vqdSuspension">
    <view class="element suspension" style="bottom: {{data.bottom}}rpx;">
        <view>
            <view class="suspension-item" wx:if="{{data.choose_service}}" style="{{data.style}}">
                <icon class="ico-moon icon-service suspension-service">
                    <contact-button type="default-dark" size="20" session-from="suspension"></contact-button>
                </icon>
            </view>
            <block wx:for="{{data.content}}" wx:key="{{item}}">
                <view class="suspension-item suspDiy" style="{{data.style}}" data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
                    <image style='{{data.imgsize}}' src='{{item.pic}}'></image>
                </view>
            </block>
        </view>
