                    data-score="2" bindtap="bindScoreChange"></label>
                <label class="ico-moon icon-{{data.icon == 'love' ? 'heart' : (data.icon == 'plum' ? 'clubs' : data.icon)}}{{ data.editScore >= 3 ? '' : '-empty' }}" data-compid="{{data.compId}}" data-segment="{{data.segment}}" data-formcompid="{{data.formCompid}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}"
                    data-score="3" bindtap="bindScoreChange"></label>
