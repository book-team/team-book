<!--商家列表-->
<template name="shoplist">
    <view class="element franchisee-list" style="{{data.style}}">
        <view class="element search" wx:if="{{data.search}}">
            <view class="search-input" data-compid="{{data.compid}}" catchtap="searchShop">
                <icon type="search" size="16"></icon>
                <text>{{data.searchText}}</text>
            </view>
        </view>
        <view class="franchisee-title">
            <view class="franchisee-location" data-compid="{{data.compid}}" data-nid="{{data.listid}}" bindtap="tapFranchiseeLocation">
                <label class="ico-moon icon-dizhi"></label> {{ data.location_address || '定位中...' }}</view>附近商家
        </view>
        <scroll-view class="scroll-ele" data-compid="{{data.compid}}" scroll-y="true" style="{{data.scorllHei+data.style}}" data-pagenum="{{data.pagenum||0}}" bindscrolltolower="loadMoreShop" data-nid="{{data.listid}}">
