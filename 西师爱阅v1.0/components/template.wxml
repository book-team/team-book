                    </view>
                </picker>
                <view class="check-time-one check-time-cen">
                    <view class="line-view"></view>
                    <view class="check-text" style="{{data.constyle}}">共</view>
                    <view class="check-days" style="{{data.textstyle}}">{{data.night}}</view>
                    <view class="check-text" style="{{data.constyle}}">晚</view>
                    <view class="line-view"></view>
                </view>
                <picker class="hotel-date-picker" mode="date" start="{{data.leave_start}}" end="{{data.leave_range}}" data-compid="{{data.compid}}" data-type="out" data-enter="2" data-from="search" bindchange="bindHoutelPicker">
                    <view class="check-time-one">
                        <view class="check-view">
                            <view class="check-title" style="{{data.constyle}}">离店日期</view>
                            <view class="check-date" style="{{data.constyle}}">{{data.outDate}}</view>
                        </view>
                    </view>
                </picker>
            </view>
            <view class="search_button" style="{{data.bacstyle}}" catchtap="gotoHotelSearch" data-type="2" data-compid="{{data.compid}}">搜索</view>
        </view>
    </view>
</template>
