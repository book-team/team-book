                        <view class="list-item background-ele" style="{{data.listStyle}}" data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
                            <view class="title-container newshequ-title" style="{{data.titleWidth}}">
                                <view class="title" style="{{data.titlefont}}">{{item.title}}</view>
                                <view class="sec-title" style="{{data.abstractfont}}">作者：{{item.topicNickName}} 评论：{{item.comment}}</view>
                            </view>
                        </view>
