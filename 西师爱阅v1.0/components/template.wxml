<!--引入文件  -->
<import src="/components/wxParse/wxParse.wxml" />
<!--文本-->
<template name="vqdText">
    <block wx:if="{{real_value}}">
        <block wx:if="{{real_value[0].type === 'obj'}}">
            <view class="element text" data-text="{{real_value}}" style="{{data.style}}" data-event-params="{{data.eventParams}}" data-phoneseg="{{phoneseg}}" catchtap="{{data.eventHandler}}">
                <view class="wxParse">
                    <template is="wxParse" data="{{wxParseData: real_value}}" />
                </view>
            </view>
        </block>
        <block wx:else>
            <text class="element text" data-text="{{real_value}}" style="{{data.style}}" data-event-params="{{data.eventParams}}" data-phoneseg="{{phoneseg}}" catchtap="{{data.eventHandler}}">{{real_value}}</text>
        </block>
    </block>
    <block wx:else>
        <text class="element text" data-text="{{data.content}}" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">{{data.content}}</text>
    </block>
</template>
<!--图片-->
<template name="vqdPicture">
    <block wx:if="{{real_value}}">
        <block wx:if="{{real_value[0].type === 'obj'}}">
            <view class="element picture" style="{{data.style}}height: auto;font-size: 0;" data-event-params="{{data.eventParams}}" data-phoneseg="{{phoneseg}}" catchtap="{{data.eventHandler}}">
                <view class="wxParse">
                    <template is="wxParse" data="{{wxParseData: real_value}}" />
                </view>
            </view>
        </block>
        <block wx:else>
            <view class="element picture" data-phoneseg="{{phoneseg}}" style="{{data.style}}height: auto;font-size: 0;" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
                <image src="{{real_value}}" lazy-load="true" style="{{data.imgstyle}}"></image>
            </view>
        </block>
    </block>
    <block wx:else>
        <view class="element picture" style="{{data.style}}height: auto;font-size: 0;" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
            <image src="{{data.content.pic}}" lazy-load="true" style="{{data.imgstyle}}"></image>
        </view>
    </block>
    <view class="shop_remove_bg" hidden="{{!showNcardBg}}"></view>
    <view class="rule_dialog" hidden="{{!showNcard}}">
        <view class="privilege_list">
            <text>{{ncardmsg}}</text>
        </view>
        <view class="close_rule" catchtap="closeRule">
            <image lazy-load="true" src="https://www.vqida.com/static/user/images/signclose.png" style="width: 31rpx;height: 31rpx;"></image>
        </view>
    </view>
</template>
<!--按钮-->
<template name="vqdButton">
    <block wx:if="{{real_value}}">
        <block wx:if="{{real_value[0].type === 'obj'}}">
            <view class="element button" data-phoneseg="{{phoneseg}}" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
                <view class="wxParse">
                    <template is="wxParse" data="{{wxParseData: real_value}}" />
                </view>
            </view>
        </block>
        <block wx:else>
            <button class="element button" data-phoneseg="{{phoneseg}}" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
                {{real_value}}
            </button>
        </block>
    </block>
    <block wx:else>
        <button class="element button" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
            {{data.content}}
        </button>
    </block>
</template>
<!--标题-->
<template name="vqdTitle">
    <view class="element title-ele {{ 'title-mode'+ data.mode }}" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
        <view class="title-content">
            <view class="title-text">
                <text class="mark" style="{{'background-color:' + data.markColor}}"></text>
                <text class="vqd-text">{{data.content}}</text>
            </view>
        </view>
    </view>
</template>
<!--轮播-->
<template name="vqdCarousel">
    <swiper indicator-dots="{{data.content.length>1}}" autoplay="{{data.autoplay}}" interval="{{data.interval*1000}}" duration="1000" circular="true" style="{{data.style}}">
        <block wx:for="{{data.content}}" wx:key="{{item}}">
            <swiper-item data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
                <image class="swiper-item" src="{{item.picpath}}" />
            </swiper-item>
        </block>
    </swiper>
</template>
<!--分类-->
<template name="vqdClassify">
    <view class="element classify {{'classify-mode'+data.mode }}" style="{{data.style}}">
        <scroll-view scroll-x="true">
            <block wx:for="{{data.content}}" wx:for-index="idx" wx:for-item="itemName" wx:key="{{itemName}}">
                <block wx:if="{{idx==data.selected}}">
                    <block wx:if="{{data.mode==1}}">
                        <view class="classify-item selected" data-event-params="{{itemName.eventParams}}" catchtap="{{itemName.eventHandler}}" style="{{data.mleft}};color:{{data.selectedColor}}" data-idx='{{idx}}' data-compid="{{data.compid}}">
                            <text>{{itemName.text}}</text>
                            <i class="underline" style="border-color:{{data.selectedColor}}"></i>
                        </view>
                    </block>
                    <block wx:else>
                        <view class="classify-item selected" data-event-params="{{itemName.eventParams}}" catchtap="{{itemName.eventHandler}}" style="{{data.mleft}};color:{{data.selectedColor}}" data-idx='{{idx}}' data-compid="{{data.compid}}">
                            <text style="background-color:{{data.selectedColor}};border-radius: {{data.radius||0}}px;">{{itemName.text}}</text>
                        </view>
                    </block>
                </block>
                <block wx:else>
                    <view style="{{data.mleft}};color:{{data.unselectColor}}" class="classify-item" data-event-params="{{itemName.eventParams}}" catchtap="{{itemName.eventHandler}}" data-idx='{{idx}}' data-compid="{{data.compid}}">
                        <text>{{itemName.text}}</text>
                    </view>
                </block>
            </block>
        </scroll-view>
    </view>
</template>
<!--图片列表-->
<template name="vqdAlbum">
    <view class="element album {{data.html_mode}}" style="{{data.style}}">
        <view class="album-container background-ele clearfix" style="{{data.ul_style}} ">
            <view wx:for="{{data.li}}" wx:key="{{item}}" class="{{item.li_class}}" style="{{item.li_style}}" data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
                <image lazy-load="true" src="{{item.pic}}" style="{{item.img_style}}" mode="scaleToFill"></image>
                <text class="title" style="{{item.text_style}}">{{item.title}}</text>
            </view>
        </view>
    </view>
</template>
<!--图文集-->
<template name="vqdList">
    <view class="element list" style="{{data.style}}">
        <view class="list-container {{data.sec_mode}}">
            <block wx:for="{{data.content}}" wx:key="{{item}}">
                <view class="list-item background-ele" style="{{item.listStyle}}" data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
                    <image lazy-load="true" class="list-img" src="{{item.pic}}" style="{{item.imgStyle}}"></image>
                    <view class="title-container" style="margin-left: 23rpx;{{item.titleWidth}}">
                        <view class="title" style="margin-bottom: 12rpx;{{item.titlefont}}">{{item.title}}</view>
                        <view class="sec-title" style="text-indent: 0;{{item.abstractfont}}">{{item.secTitle}}</view>
                    </view>
                </view>
            </block>
        </view>
    </view>
</template>
<!--视频-->
<template name="vqdVideo">
    <view class="element video" style="{{data.style}}">
        <video src="{{data.videourl}}" poster="{{data.poster}}"></video>
    </view>
</template>
<!--分割线-->
<template name="vqdBreakline">
    <view class="element breakline" style="{{data.style}}"></view>
</template>
<!--用户中心-->
<template name="vqdUserCenter">
    <view class="user-center usercenter-mode1 background-ele" style="{{data.style}}">
        <view class="show-view" style="background: url('{{data.piccontent.pic}}') center no-repeat;">
            <view class="personal_img_box">
                <image lazy-load="true" class="cover-thumb" src="{{userInfo.avatarUrl||'https://www.vqida.com/static/user/images/portrait.png'}}"></image>
            </view>
            <view class="personal_name_box">
                <text class="personal_name" style="{{data.namestyle}}">{{userInfo.nickName||'昵称'}}</text>
            </view>
            <block wx:for="{{data.content}}" wx:key="{{item}}">
                <block wx:if="{{item.choose && item.text=='积分签到'}}">
                    <view class="personal_name_box">
                        <text class="sign_span">积分{{total}}</text>
                        <text class="sign_span" catchtap="pointSign">签到</text>
                    </view>
                </block>
            </block>
            <view class="signbg" hidden="{{!sign}}">
                <view class="signdialog">
                    <image lazy-load="true" src="https://www.vqida.com/static/user/images/signdialog.png" style="width: 594rpx;height: 420rpx;position: absolute;top: 0;left: 0;"></image>
                    <view class="signcontent">
                        <view class="success">
                            <text>{{promitTitle}}</text>
                        </view>
                        <view class="obtainpoint">
                            <text>{{integralpromit}}</text>
                        </view>
                        <button class="sharebtn" open-type="share">
                            <image lazy-load="true" src="https://www.vqida.com/static/user/images/sharebg.png" style="width: 275rpx;height: 85rpx;position: absolute;top: 0;left: 0;"></image>
                            <text style="position: relative;z-index: 10000000;">分享给好友</text>
                        </button>
                    </view>
                    <view class="closedialog" catchtap="signClose">
                        <image lazy-load="true" src="https://www.vqida.com/static/user/images/signclose.png" style="width: 31rpx;height: 31rpx;"></image>
                    </view>
                </view>
            </view>
        </view>
        <view class="horizontal-view" style="font-weight: normal;font-style: normal;text-decoration:none;">
            <view class="horizontal-router-container1">
                <block wx:for="{{data.content}}" wx:key="{{item}}">
                    <view wx:if="{{item.choose && item.text!='我要分销' && item.text!='积分签到' && item.text!='我的同城' && item.text!='预约中心'}}">
                        <view style="width: 100%;" data-name="{{item.text}}" data-typeidx="0" catchtap="UserCenterPage">
                            <label class="ico-moon {{item.pic}}"></label>
                            <text>{{item.text}}</text>
                            <view class="rightarrow">
                                <text wx:if="{{item.text=='我的订单' || item.text=='全部订单'}}">查看全部</text>
                                <label class="ico-moon icon-rightarrow"></label>
                            </view>
                        </view>
                        <view class="ordertype-box clearfix" wx:if="{{item.text=='我的订单' || item.text=='全部订单'}}">
                            <view class="ordertype-list" data-name="{{item.text}}" data-typeidx="1" catchtap="UserCenterPage">
                                <view style="display:inline-block;border: 0;padding: 0;">
                                    <image lazy-load="true" src="https://www.vqida.com/static/user/images/payment_img.png"></image>
                                </view>
                                <view class="ordertype-view">待付款</view>
                            </view>
                            <view class="ordertype-list" data-name="{{item.text}}" data-typeidx="2" catchtap="UserCenterPage">
                                <view style="display:inline-block;border: 0;padding: 0;">
                                    <image lazy-load="true" src="https://www.vqida.com/static/user/images/deliver_img.png"></image>
                                </view>
                                <view class="ordertype-view">待发货</view>
                            </view>
                            <view class="ordertype-list" data-name="{{item.text}}" data-typeidx="3" catchtap="UserCenterPage">
                                <view style="display:inline-block;border: 0;padding: 0;">
                                    <image lazy-load="true" src="https://www.vqida.com/static/user/images/receipt_img.png"></image>
                                </view>
                                <view class="ordertype-view">待收货</view>
                            </view>
                            <view class="ordertype-list" data-name="{{item.text}}" data-typeidx="4" catchtap="UserCenterPage">
                                <view style="display:inline-block;border: 0;padding: 0;">
                                    <image lazy-load="true" src="https://www.vqida.com/static/user/images/evaluate_img.png"></image>
                                </view>
                                <view class="ordertype-view">待评价</view>
                            </view>
                        </view>
                    </view>
                </block>
                <block wx:for="{{data.content}}" wx:key="{{item}}">
                    <view wx:if="{{item.choose && item.text=='我要分销' && (disgrade==1||disgrade==2)}}">
                        <view style="width: 100%;" data-name="{{item.text}}" data-typeidx="0" catchtap="UserCenterPage">
                            <label class="ico-moon {{item.pic}}"></label>
                            <text>{{item.text}}</text>
                            <view class="rightarrow">
                                <label class="ico-moon icon-rightarrow"></label>
                            </view>
                        </view>
                    </view>
                </block>
                <block wx:for="{{data.content}}" wx:key="{{item}}">
                    <view wx:if="{{item.choose && item.text=='我的同城'}}" style="border-top: 6px solid #eee;">
                        <view style="width: 100%;" data-name="{{item.text}}" data-typeidx="0" catchtap="UserCenterPage">
                            <label class="ico-moon {{item.pic}}"></label>
                            <text>{{item.text}}</text>
                            <view class="rightarrow">
                                <label class="ico-moon icon-rightarrow"></label>
                            </view>
                        </view>
                    </view>
                </block>
                <block wx:for="{{data.content}}" wx:key="{{item}}">
                    <view wx:if="{{item.choose && item.text=='预约中心'}}" style="border-top: 6px solid #eee;">
                        <view style="width: 100%;" data-name="{{item.text}}" data-typeidx="0" catchtap="UserCenterPage">
                            <label class="ico-moon {{item.pic}}"></label>
                            <text>{{item.text}}</text>
                            <view class="rightarrow">
                                <label class="ico-moon icon-rightarrow"></label>
                            </view>
                        </view>
                    </view>
                </block>
            </view>
        </view>
    </view>
    <view class="shop_remove_bg" hidden="{{!showNcardBg}}"></view>
    <view class="rule_dialog" hidden="{{!showNcard}}">
        <view class="privilege_list">
            <text>{{ncardmsg}}</text>
        </view>
        <view class="close_rule" catchtap="closeRule">
            <image lazy-load="true" src="https://www.vqida.com/static/user/images/signclose.png" style="width: 31rpx;height: 31rpx;"></image>
        </view>
    </view>
</template>
<!--音乐-->
<template name="vqdAudio">
    <view class="element audio">
        <audio controls loop="true" poster="{{ data.thumb }}" name="{{ data.name}}" src="{{data.src}}" author="{{data.author}}"></audio>
    </view>
</template>
<!--计数-->
<template name="vqdCountEle">
    <block wx:if="{{data.total || data.total == 0}}">
        <view class="element count-ele" style="{{data.style}}">
            <label class="count-icon {{data.icon}} active" style="width: {{data.icon_size}}; height: {{data.icon_size}};background-size:auto 100% !important" data-autocount="{{data.autocount}}" data-countid="{{data.countid}}" data-compid="{{data.compid}}" data-total="{{data.total}}"
                data-icon="{{data.icon}}" data-isclick="{{data.isclick}}" catchtap="changeCount"></label>
            <label class="count-tip" data-autocount="{{data.autocount}}" data-countid="{{data.countid}}" data-compid="{{data.compid}}" data-total="{{data.total}}" data-icon="{{data.icon}}" data-isclick="{{data.isclick}}" catchtap="changeCount">{{data.content}}</label>
            <label class="vqd-count-value" data-autocount="{{data.autocount}}" data-countid="{{data.countid}}" data-compid="{{data.compid}}" data-total="{{data.total}}" data-icon="{{data.icon}}" data-isclick="{{data.isclick}}" catchtap="changeCount">{{data.total}}</label>
        </view>
    </block>
</template>
<!--地图-->
<template name="vqdMap">
    <view class="element map" style="{{data.style}}">
        <map class="map-module" hidden="{{data.showmap != 1}}" longitude="{{data.lng}}" latitude="{{data.lat}}" markers="{{data.markers}}"></map>
        <view class="map-link">{{data.content}}</view>
    </view>
</template>
<!--排序-->
<template name="vqdSort">
    <view class="element sort" style="{{data.style}}">
        <scroll-view scroll-x="true" style="{{data.ul_style}}">
            <block wx:for="{{data.content}}" wx:for-index="idx" wx:for-item="itemName" wx:key="{{itemName}}">
                <block wx:if="{{idx == data.selected}}">
                    <view class="item refresh selected" catchtap="sortListFunc" style="color:{{data.selectedColor}}">
                        <text>{{itemName.text}}</text>
                        <label class="arr-wrap" wx:if="{{ idx != 0 }}">
                            <text style="color: {{itemName.sort_direction == 0 ? data.selectedColor : '#ddd'}};" class="sort-arr arr-up">▲</text>
                            <text style="color: {{itemName.sort_direction == 1 ? data.selectedColor : '#ddd'}};" class="sort-arr arr-down">▼</text>
                        </label>
                    </view>
                </block>
                <block wx:else>
                    <view class="item refresh" catchtap="sortListFunc">{{itemName.text}}</view>
                </block>
            </block>
        </scroll-view>
    </view>
</template>
<!--评论-->
<template name="vqdComment">
    <view class="element" style="display:inline-block;background:#fff;{{data.style}}">
        <form bindsubmit="submitComment" data-compId="{{data.compid}}" class="commentform" data-cid="{{data.comment_id}}" data-cname="{{data.commentName}}">
            <textarea placeholder="{{data.placeholder||'我也来说几句...'}}" class="commentbox" placeholder-class="commentbox-plh" bindinput="commentChange" data-compId="{{data.compid}}" value="{{data.commentText}}" />
            <button class="commentbtn" formType="submit">发 布</button>
        </form>
        <block wx:if="{{data.total&&data.total>0}}">
            <view class="commentlist">
                <view class="commentlist-title">
                    最新评论（{{data.total||0}}）
                </view>
                <block wx:for="{{data.commentlist}}" wx:key="{{item}}">
                    <view class="commentlist-content">
                        <view class="comment-one">
                            <image lazy-load="true" src="{{item.headurl}}" class="userhead"></image>
                            <text class="usernickname">{{item.username}}</text>
                            <text class="ico-moon icon-heart comment-praise" catchtap="commentZan" data-id="{{item.commentId}}" data-datakey="{{data.compid}}.commentlist[{{index}}]"> {{item.zan}}</text>
                            <text class="comment-time">{{item.addtime}}</text>
                        </view>
                        <view class="comment-one">
                            <text class="comment-text">{{item.text}}</text>
                            <block wx:if="{{!item.replyshow}}">
                                <button class="ico-moon icon-replay reply-master" catchtap="showReply" data-id="{{item.commentId}}" data-compid="{{data.compid}}" data-idx="{{index}}" data-needload="{{item.needload}}"> 回复
                                    <block wx:if="{{item.replaynum>0}}"> +{{item.replaynum}}</block>
                                </button>
                            </block>
                        </view>
                        <block wx:if="{{item.replyshow}}">
                            <view>
                                <view class="arrow">
                                    <view class="reply-arrow"></view>
                                </view>
                                <block wx:for="{{item.content}}" wx:for-item="oneitem" wx:for-index="oneindex" wx:key="{{oneitem}}">
                                    <view class="comment-reply">
                                        <view class="comment-one">
                                            <image lazy-load="true" src="{{oneitem.headurl}}" class="replyhead"></image>
                                            <text class="usernickname">{{oneitem.username}}</text>
                                            <text class="ico-moon icon-heart comment-praise" catchtap="commentZan" data-id="{{oneitem.commentId}}" data-datakey="{{data.compid}}.commentlist[{{index}}].content[{{oneindex}}]"> {{oneitem.zan}}</text>
                                            <text class="comment-time">{{oneitem.addtime}}</text>
                                        </view>
                                        <view class="comment-one">
                                            <text class="comment-reply-text">{{oneitem.text}}</text>
                                            <button class="reply-other" catchtap="clickReply" data-datakey="{{data.compid}}.commentlist[{{index}}]" data-text="回复 {{oneitem.username}}：">回复</button>
                                        </view>
                                    </view>
                                </block>
                                <view class="reply-area">
                                    <button class="reply-first" catchtap="clickReply" data-datakey="{{data.compid}}.commentlist[{{index}}]" data-text="">我也说一句</button>
                                    <form bindsubmit="submitReply" class="eplay-area-enter" data-compid="{{data.compid}}" data-idx="{{index}}" data-id="{{item.commentId}}" data-cid="{{data.comment_id}}" data-cname="{{data.commentName}}">
                                        <input class="reply-input" bindinput="replyChange" data-datakey="{{data.compid}}.commentlist[{{index}}]" value="{{item.replyContent}}" />
                                        <button class="comment-sendbtn" formType="submit">发 送</button>
                                    </form>
                                </view>
                                <view class="comment-operate">
                                    <block wx:if="{{item.remianNum && item.remianNum>0}}">
                                        <view class="operate-more">
                                            还有{{item.remianNum||0}}条回复，
                                            <text class="ico-moon icon-play_arrow click-more" catchtap="showReply" data-id="{{item.commentId}}" data-compid="{{data.compid}}" data-idx="{{index}}" data-needload="false">点击查看</text>
                                        </view>
                                    </block>
                                    <view class="operate-hide ico-moon icon-arrow-top" catchtap="hideReply" data-compid="{{data.compid}}" data-idx="{{index}}">收起回复</view>
                                </view>
                            </view>
                        </block>
                    </view>
                </block>
            </view>
            <block wx:if="{{data.remainNum != 0 && data.remainNum}}">
                <view class="ico-moon inco-loadmore loadmore" catchtap="loadmore" data-compid="{{data.compid}}" data-cid="{{data.comment_id}}"></view>
            </block>
        </block>
    </view>
</template>
<!--动态文章-->
<template name="vqdDynamiclist">
    <view class="element list" style="{{data.style}}">
        <view class="element search" wx:if="{{data.search}}">
            <view class="search-input" data-compid="{{data.compid}}" catchtap="searchArticle">
                <icon type="search" size="16"></icon>
                <text>{{data.searchText}}</text>
            </view>
        </view>
        <block wx:if="{{data.content.length>0}}">
            <view class="list-container {{data.sec_mode}}">
                <block wx:for="{{data.content}}" wx:key="{{item}}">
                    <view class="list-item background-ele {{data.choosestyle?'newteletextli':''}}" style="{{data.listStyle}}" data-id="{{item.id}}" catchtap="goToArt">
                        <image lazy-load="true" class="list-img" src="{{item.picpath}}" style="{{data.imgStyle}} left: 0;top: 0;"></image>
                        <view class="teletext_text_bg" style="{{data.textbg}}"></view>
                        <view class="title-container article-title {{data.choosenum!=3?'titletop1':'titletop2'}}" style="{{data.titleWidth}}">
                            <view class="title" style="{{data.titlefont}}">{{item.simpletitle}}</view>
                        </view>
                        <view class="teletext_text_bottom clearfix {{data.choosenum==3?'titletop2':''}}" style="{{data.bottomstyle}}">
                            <view class="teletext_text_date" style="{{data.datestyle}}" wx:if="{{data.showTime}}">{{item.addtime}}</view>
                            <view class="teletext_text_num" wx:if="{{data.showBrowse}}">
                                <image lazy-load="true" src="{{data.numpic.pic}}" style="display: inline-block !important;margin-top: 0 !important;"></image>
                                <text style="{{data.numstyle}}">{{item.rednums}}</text>
                            </view>
                        </view>
                        <label class="ico-moon icon-recommend" wx:if="{{item.recommend==1}}"></label>
                    </view>
                </block>
            </view>
            <view class="clickmore" wx:if="{{data.havenums>0 && data.loaddata && data.datatype==2}}" data-compid="{{data.compid}}" catchtap="loadmoreArt">加载更多</view>
        </block>
        <block wx:else>
            <view wx:if="{{data.shownum>0}}" style="text-align:center;padding:50rpx;" hidden='{{!data.nodata}}'>亲~暂无内容，谢谢！</view>
        </block>
    </view>
</template>
<!--动态表单--单文本-->
<template name="vqdInputEle">
    <block wx:if="{{data.namestyle}}">
        <view class="element input-ele vqd-form-ele" style="{{data.style}}">
            <text class="single_text_span" style="{{data.namestyle}}"><text wx:if="{{data.isneed==2}}" class="form_must" wx:if="{{data.isneed==2}}">*</text>{{data.textname}}</text>
            <input type="text" placeholder="{{data.content}}" data-segment="{{data.segment}}" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" bindinput="inputChange" style="{{data.contentstyle}}"
            />
        </view>
    </block>
    <block wx:else>
        <view class="element input-ele vqd-form-ele" style="{{data.style}}">
            <input type="text" placeholder="{{data.content}}" data-segment="{{data.segment}}" data-compid="{{data.compId}}" data-formcompid="{{data.formCompid}}" data-datakey="{{data.formCompid}}.form_data.{{data.segment}}" bindinput="inputChange" />
        </view>
    </block>
</template>
