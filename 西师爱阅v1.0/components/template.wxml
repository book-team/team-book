                    <text style="{{data.textstyle}}">附近</text>
                </view>
            </view>
            <view class="check-time-box" style="background: none">
                <picker class="hotel-date-picker" mode="date" start="{{data.now_time}}" end="{{data.enter_range}}" data-compid="{{data.compid}}" data-type="in" data-enter="1" data-from="search" bindchange="bindHoutelPicker">
