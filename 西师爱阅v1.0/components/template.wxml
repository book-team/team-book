                            <text class="{{data.selnid==item.nid?'hotel_class':''}}" data-compid='{{data.compid}}' data-nid="{{item.nid}}" catchtap='hotelTap'>{{item.name}}</text>
                        </block>
                    </scroll-view>
                    <view class="hotel_text">
                        <text data-compid='{{data.compid}}' data-type='1' catchtap='sure_hotel_nid'>取消</text>
                        <text data-compid='{{data.compid}}' data-type='2' catchtap='sure_hotel_nid'>确定</text>
                    </view>
                </view>
            </block>
        </view>
        <block wx:if="{{data.content.length>0}}">
            <block wx:for="{{data.content}}" wx:key="{{item}}">
                <view class="hoList_detail" catchtap='gotoHotel' data-childid='{{item.child_id}}'>
                    <image src='{{item.logo}}'></image>
                    <view class="hod_name">
                        <text style="{{data.hotelstyle}}">{{item.storename}}</text>
                        <text style="{{data.pricestyle}}">{{item.lowprice}}</text>
                    </view>
                    <view class="hod_condition">
                        <text>{{item.get_score}}分 </text>
                        <text> {{item.commentPeople}}人评价</text>
                        <block wx:if="{{item.is_luggage==1}}">
                            <text>寄存</text>
                        </block>
                        <block wx:if="{{item.special_eat==1}}">
                            <text>餐厅</text>
                        </block>
                        <block wx:if="{{item.free_wifi==1}}">
                            <text>Wifi</text>
                        </block>
                        <block wx:if="{{item.free_park==1}}">
                            <text>停车</text>
                        </block>
                    </view>
                    <view class="hod_address">
                        <text style="{{data.distancestyle}}">{{item.address}}{{item.detailaddress}}</text>
                        <text style="{{data.distancestyle}}">{{item.range}}</text>
                    </view>
                </view>
            </block>
            <view class="clickmore" wx:if="{{data.haveNums>0 && data.loaded && data.datatype==2}}" data-compid="{{data.compid}}" catchtap="loadmoreHotel">加载更多</view>
        </block>
        <block wx:else>
            <view wx:if="{{data.nothing}}" style="text-align:center;line-height: 100rpx;">亲~暂无内容，谢谢！</view>
        </block>
    </view>
</template>
<!-- 酒店搜索 -->
<template name="vqdhotelso">
    <view class="seckill-content" style="position:relative;" style="{{data.style}}">
        <view class="search_content">
            <view class="search_title " style="{{data.datestyle}}">
