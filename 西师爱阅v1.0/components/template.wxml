<!--商品列表-->
<template name="goodslist">
    <view hidden="{{!data.loaded}}">
        <view class="element list" style="{{data.style}}">
            <view class="element search" wx-if="{{data.search}}">
                <view class="search-input" data-compid="{{data.compid}}" catchtap="searchGoods" data-bindobj="{{data.bindObj}}">
                    <icon type="search" size="16"></icon>
                    <text>{{data.searchText}}</text>
                </view>
            </view>
            <view class="order_list" wx-if="{{data.order}}">
                <view class="order_item {{data.ordertype==1?'order_checked':''}}" data-compid='{{data.compid}}' data-sel='1' catchtap='sel_goods_type'>
                    <text class="items">综合</text>
                </view>
                <view class="order_item {{data.ordertype==2?'order_checked':''}}" data-compid='{{data.compid}}' data-sel='2' catchtap='sel_goods_type'>
                    <text class="items">销量</text>
                </view>
                <view class="order_item {{data.ordertype==3||data.ordertype==4?'order_checked':''}}" data-compid='{{data.compid}}' data-sel='3' catchtap='sel_goods_type'>
                    <text class="items">价格</text>
                    <view class="order_adjust" hidden='{{data.ordertype!=3&&data.ordertype!=4}}'>
                        <text class="order_up ico-moon icon-up {{data.ordertype==3?'select':''}}"></text>
                        <text class="order_down ico-moon icon-tripple-down {{data.ordertype==4?'select':''}}"></text>
                    </view>
                </view>
                <view class="order_item {{data.ordertype==5?'order_checked':''}}" data-compid='{{data.compid}}' data-sel='5' catchtap='sel_goods_type'>
                    <text class="items">新品</text>
                </view>
            </view>
            <scroll-view class="list-container {{data.mode==4?'good_list_ul':''}}" scroll-x="{{data.mode==4}}" scroll-y="{{data.mode!=4}}" bindscrolltolower="scrollMoreGood" data-compid='{{data.compid}}' data-nid='{{data.listid}}'>
                <block wx:if="{{data.content.length>0}}">
                    <block wx:for="{{data.content}}" wx:key="{{item}}">
                        <navigator url="/dianshang/goodsDetail/goodsDetail?id={{item.id}}&bindObj={{data.bindObj}}" class="{{data.mode==4?'scroll-left':''}}">
                            <view class="list-item background-ele goodType{{data.mode}} {{data.mode==2?'double-list':''}}" style="{{data.listStyle}}">
                                <image lazy-load="true" class="list-img" src="{{item.picpath}}" style="{{data.imgStyle}}"></image>
                                <view class="title-container" style="{{data.titleWidth}}">
                                    <view class="title goodsName{{data.mode}}" style="{{data.titlefont}}">{{item.goodname}}</view>
                                    <view class="sec-title">
                                        <text style="{{data.pricestyle}}">￥{{item.price}}</text>
                                        <text style="{{data.salestyle}}">销量:{{item.salesnums}}</text>
                                    </view>
                                </view>
                                <label class="ico-moon icon-recommend" wx:if="{{item.recommend==1}}"></label>
                            </view>
                        </navigator>
                    </block>
                </block>
                <block wx:else>
                    <view hidden="{{!data.areanothing}}" style="text-align:center;padding:50rpx;">亲~该区域暂无商品上架，谢谢。</view>
                    <view wx:if="{{data.shownum>0&&!data.areanothing&&data.nothing}}" style="text-align:center;padding:50rpx;">亲~暂无内容，谢谢！</view>
                </block>
            </scroll-view>
            <view class="clickmore" wx:if="{{data.havenums>0 && data.loaddata && data.datatype==2 && data.mode!=4}}" data-compid="{{data.compid}}" data-nid="{{data.listid}}" catchtap="loadmoreGoods">加载更多</view>
        </view>
    </view>
</template>
<!--城市定位-->
<template name="citylocation">
    <view class="element citylocation" style="{{data.style}}" data-id="{{data.compid}}" wx:if="{{data.bindIdx && data.bindIdx!=-1}}">
        <view class="citylocation-input" bindtap="selectLocal" data-id="{{data.compid}}">
            <text data-compid="{{data.compid}}">当前选择城市:{{ data.district ? data.province+' '+data.city+' '+data.district : data.local }} </text>
        </view>
    </view>
</template>
<!--选择城市-->
<template name="citylocation_select">
    <view class="citylocation-choose" hidden="{{!data.hidden}}">
        <view>
            <view class="picker-btn">
                <text bindtap="cancelCity" data-id="{{data.compid}}">取消</text>
                <text bindtap="submitCity" data-id="{{data.compid}}" data-glist="{{data.bindlist}}">确定</text>
            </view>
            <picker-view indicator-style="height: 50px;" style="width: 100%; height: 300px;" value="{{data.value}}" bindchange="bindCityChange" data-id="{{data.compid}}">
                <picker-view-column>
                    <view wx:for="{{data.provinces}}" style="line-height: 50px" wx:key="{{item}}">{{item}}</view>
                </picker-view-column>
                <picker-view-column>
                    <view wx:for="{{data.citys}}" style="line-height: 50px" wx:key="{{item}}">{{item}}</view>
                </picker-view-column>
                <picker-view-column>
                    <view wx:for="{{data.districts}}" style="line-height: 50px" wx:key="{{item}}">{{item}}</view>
                </picker-view-column>
            </picker-view>
        </view>
    </view>
</template>
<!--分享按钮-->
<template name="shareButton">
    <button class="element button" style="{{data.style}}" open-type="share">
        <image lazy-load="true" src="{{data.content.pic}}" class="shareIcon" style="{{data.iconStyle}}"></image>
        {{data.content.text}}
